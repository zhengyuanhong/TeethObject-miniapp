.box{
    width: 95%;
    margin: 0rpx auto;
}

.box .name{
 background: white;
 margin-top: 20rpx;
 padding: 20rpx 30rpx;
 font-size: 30rpx;
 border-radius: 10rpx;
}

.company_info{
    display: flex;
    background: white;
    margin: 20rpx auto;
    padding: 20rpx;
    border-radius: 10rpx;
    align-items: center;
  }
  
  .company_info .left image{
    width: 100rpx;
    height: 100rpx;
  }
  
  .company_info .right{
    margin-left: 20rpx;
  }

  .company_info .right .company_name{
      font-weight: bolder;
      font-size: 35rpx;
  }

  .company_info .right .juli{
    margin-top: 10rpx;
}
